{
  "name": "{{org}}-root",
  "version": "0.0.0",
  "private": true,
  "description": "Default description for {{org}}",
  "repository": "git@github.com:elmpp/{{org}}.git",
  "author": "matthew.penrice@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elmpp/{{org}}/issues"
  },
  "scripts": {
    "contributors-generate": "all-contributors generate",
    "format": "eslint . --fix",
    {{#if splittable}}
    "lint": "eslint . && yarn workspaces run lint",
    "postinstall": "echo ''",
    {{else}}
    "postinstall": "cd ../.. && yarn postinstall",
    {{/if}}
    "test": "jest --passWithNoTests"
  },
  {{#if splittable}}
  "workspaces": [
    "packages/*"
  ],
  {{/if}}
  "engines": {
    "node": ">=12.14.0"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@commitlint/config-lerna-scopes": "8.2.0",
    "@types/lodash": "4.14.149",
    "@types/jest": "24.0.23",
    "@types/react": "16.8.23",
    "all-contributors-cli": "6.11.2",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "jest": "24.9.0",
    "patch-package": "6.2.0",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "typescript": "3.7.4",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "@typescript-eslint/parser": "2.13.0"
  }
}
